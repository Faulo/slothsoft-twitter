{
    "name": "slothsoft/twitter",
    "description": "A Farah module that downloads twitter feeds of individual users.",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Daniel Schulz",
            "email": "info.slothsoft@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": "^7.2",
        "slothsoft/core": "dev-develop",
        "slothsoft/farah": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-0": {
            "Slothsoft\\Twitter\\": "src/"
        },
        "files": [
            "src/registerModule.php"
        ]
    },
    "keywords": [
        "slothsoft",
        "farah-module",
        "docs"
    ],
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "classmap-authoritative": false,
    "minimum-stability": "dev",
    "prefer-stable": true
}
